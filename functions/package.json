{
  "name": "functions",
  "version": "1.0.0",
  "description": "This microservice for hasura.",
  "main": "index.js",
  "author": "Pham Quy Hai",
  "license": "MIT",
  "private": true,
  "scripts": {
    "check": "gts check",
    "clean": "gts clean",
    "compile": "tsc",
    "fix": "gts fix",
    "build": "tsc --watch",
    "devHasura": "nodemon --exec npx -n='-r dotenv/config' functions-framework --port=8081 --source=./build/src/hasura",
    "devSyncS3": "nodemon --exec npx -n='-r dotenv/config' functions-framework --port=8081 --source=./build/src/sync-s3",
    "tune": "ngrok http 8081",
    "prestart": "tsc",
    "start": "yarn devHasura"
  },
  "dependencies": {
    "@google-cloud/functions-framework": "^1.7.1",
    "aws-sdk": "^2.822.0",
    "dayjs": "^1.10.2",
    "graphql": "^15.4.0",
    "graphql-request": "^3.4.0",
    "jsonwebtoken": "^8.5.1",
    "ramda": "^0.27.1",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@types/express": "^4.17.9",
    "@types/jsonwebtoken": "^8.5.0",
    "@types/node": "^14.14.20",
    "@types/ramda": "^0.27.34",
    "@types/uuid": "^8.3.0",
    "dotenv": "^8.2.0",
    "gts": "^3.0.3",
    "nodemon": "^2.0.6",
    "typescript": "^4.1.3"
  },
  "nodemonConfig": {
    "watch": [
      "build/*"
    ]
  }
}
